{
  "name": "site-spectre",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "jest",
    "build": "rm -fr dist && tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/src/index.js",
    "moving": "node dist/src/index.js https://movingfast.tech/post-sitemap.xml",
    "profiq": "node dist/src/index.js  https://www.profiq.com/wp-sitemap.xml",
    "sites": "node dist/src/index.js a -f sitesM.txt",
    "startConfig": "node dist/src/index.js -c visitConfig.json",
    "fix": "prettier --loglever warn --write .",
    "lint:prettier": "prettier --check .",
    "reinstall": "rm -fr node_modules && npm install",
    "prepare": "husky install"
  },
  "author": "",
  "license": "MIT",
  "bin": {
    "sitesp": "./dist/src/index.js"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "fast-xml-parser": "^4.2.6",
    "lodash.chunk": "^4.2.0",
    "playwright-chromium": "^1.36.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.22.5",
    "@jest/globals": "^29.6.2",
    "@types/jest": "^29.5.3",
    "@types/lodash.chunk": "^4.2.7",
    "@types/node": "^20.4.4",
    "@types/yargs": "^17.0.24",
    "husky": "^8.0.0",
    "jest": "^29.6.2",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "types.ts",
      "index.ts"
    ]
  },
  "lint-staged":{
    "**/*.{js,jsx,ts,tsx}":[
      "npx prettier --write"
    ]
  }
}
